<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cafe.ebsteam.springboard.mapper.BoardfileMapper">
	<insert id="insertBoardFile"
			parameterType="cafe.ebsteam.springboard.vo.Boardfile">     
			INSERT INTO boardfile(
				board_no,
				file_name,
				file_ext,
				file_type,
				file_size
			) VALUES (
				#{boardNo},
				#{fileName},
				#{fileExt},
				#{fileType},
				#{fileSize}
			)
	</insert>
</mapper>  


